<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- White background color -->
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="0"
            android:pivotY="0">
            <shape android:shape="rectangle">
                <solid android:color="#FFFFFF" /> <!-- White background color -->
            </shape>
        </rotate>
    </item>
</layer-list>